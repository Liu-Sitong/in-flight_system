         <center>
            <!--<MediaView fx:id="moviepane" fitHeight="${mediaViewPane.height}" fitWidth="${mediaViewPane.width}" layoutY="-8.0" />-->
             <MediaView fx:id="moviepane" fitHeight="530.0" fitWidth="800.0" nodeOrientation="INHERIT" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <!--<MediaView fx:id="moviepane" fitHeight="530.0" fitWidth="800.0" />-->
            <Pane fx:id="UIpane" prefHeight="71.0" prefWidth="480.0" BorderPane.alignment="CENTER">
